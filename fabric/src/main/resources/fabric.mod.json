{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "${mod_license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.xstopho.examplemod.ExampleMod"
        ]
    },
    "depends": {
        "fabric-api": "*",
        "fabricloader": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17",
        "resourceconfigapi": "${resource_config_api}",
        "resourcelibrary": "${resource_library}"
    }
}